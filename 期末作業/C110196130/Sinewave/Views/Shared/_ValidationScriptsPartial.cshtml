<environment include="Development">
    <script src="~/lib/js/jquery.validate.js"></script>
    <script src="~/lib/js/jquery.validate.unobtrusive.js"></script>
</environment>
<environment exclude="Development">
    <script src="https://unpkg.com/jquery-validation@1.19.5/dist/jquery.validate.min.js"
            asp-fallback-src="~/lib/js/jquery.validate.min.js"
            asp-fallback-test="window.jQuery && window.jQuery.validator"
            crossorigin="anonymous">
    </script>
    <script src="https://unpkg.com/jquery-validation-unobtrusive@3.2.12/dist/jquery.validate.unobtrusive.min.js"
            asp-fallback-src="~/lib/js/jquery.validate.unobtrusive.min.js"
            asp-fallback-test="window.jQuery && window.jQuery.validator && window.jQuery.validator.unobtrusive"
            crossorigin="anonymous">
    </script>
</environment>
